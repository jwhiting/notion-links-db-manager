{
  "name": "notion-links-db-manager",
  "version": "1.0.0",
  "description": "TypeScript application to manage Notion bookmarks database with AI-powered features",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "list-recent": "ts-node src/scripts/list-recent.ts",
    "list-tags": "ts-node src/scripts/list-tags.ts",
    "search-by-tag": "ts-node src/scripts/search-by-tag.ts",
    "search-by-any-tag": "ts-node src/scripts/search-by-any-tag.ts",
    "analyze": "ts-node src/index.ts analyze",
    "search": "ts-node src/index.ts search",
    "check-undefined-tags": "ts-node src/scripts/check-undefined-tags.ts",
    "show-tag-definitions": "ts-node src/scripts/show-tag-definitions.ts",
    "suggest-for-bookmark": "ts-node src/scripts/suggest-for-bookmark.ts",
    "suggest-for-tag": "ts-node src/scripts/suggest-for-tag.ts",
    "suggest-for-untagged": "ts-node src/scripts/suggest-for-untagged.ts",
    "list-with-ids": "ts-node src/scripts/list-with-ids.ts",
    "warm-cache-untagged": "ts-node src/scripts/warm-cache-untagged.ts",
    "warm-cache-for-tag": "ts-node src/scripts/warm-cache-for-tag.ts",
    "cache-status": "ts-node src/scripts/cache-status.ts",
    "cache-clear": "ts-node src/scripts/cache-clear.ts",
    "log:info": "echo 'INFO' > .loglevel && echo 'Log level set to INFO (verbose)'",
    "log:error": "echo 'ERROR' > .loglevel && echo 'Log level set to ERROR (clean)'",
    "log:debug": "echo 'DEBUG' > .loglevel && echo 'Log level set to DEBUG (very verbose)'",
    "log:clean": "rm -f .loglevel && echo 'Log level reset to default (clean)'",
    "log:status": "test -f .loglevel && echo \"Log level: $(cat .loglevel)\" || echo 'Log level: ERROR (default)'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "notion",
    "api",
    "bookmarks",
    "typescript",
    "database"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai": "^2.0.15",
    "@notionhq/client": "^2.2.15",
    "ai": "^5.0.15",
    "dotenv": "^16.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
